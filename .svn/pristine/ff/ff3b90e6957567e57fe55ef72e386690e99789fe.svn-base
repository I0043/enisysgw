<% if @gw_admin && params[:c1].present? && params[:c1] == "1" %>
  <% url =  system_custom_groups_path(c1: 1) %>
<% else %>
  <% url =  system_custom_groups_path %>
<% end %>
<%= render partial: 'piece_header', locals: {new: false} %>

<!-- コンテンツここから -->
<div class="main">
  <%= form_for :item, url: url, html: {method: :create, id: 'itemForm'} do |f| %>
    <%= f.error_messages %>
    <%= render partial: 'form', locals: {f: f} %>
    <%= raw(submit_for_create f, js_submitter: "return my_submit()") %>
  <% end %>
</div>
<!-- コンテンツここまで -->
