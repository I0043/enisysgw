<%= raw(gw_js_include_full) %>
<%= raw(required_head) %>

<table class="defaultTable inputTable">
<tr>
  <th><%= t("rumi.gwboard.th.listlines") %></th>
  <td><%= f.select :default_limit, @item.default_limit_circular, :class => 'select default_limit' %></td>
</tr>
<tr>
  <th><%= t("rumi.gwboard.th.default_published") %><%= required('※') %></th>
  <td class="IMEoff"><%= f.text_field :default_published, :size => 3 %><%= t("rumi.gwboard.th.between_day") %></td>
</tr>
<tr>
<th><%= t("rumi.gwboard.th.commission_limit") %><%= required('※') %></th>
<td class="IMEoff"><%= f.text_field :commission_limit, :size => 3 %></td>
</tr>
<tr>
  <th><%= t("rumi.gwboard.th.expirationdelete") %></th>
  <td><%= f.select :limit_date, @item.delete_date_name, :class => 'limit_date' %></td>
</tr>
<tr>
  <th><%= t("rumi.gwboard.th.image_size_limit") %><%= required('※') %></th>
  <td class="IMEoff"><%= f.text_field :upload_graphic_file_size_max, :size => 3 %> MB
    &nbsp;
    <%= raw(t("rumi.gwboard.message.image_size_limit"))%>
  </td>
</tr>
<tr>
  <th><%= t("rumi.gwboard.th.temp_size_limit") %><%= required('※') %></th>
  <td class="IMEoff"><%= f.text_field :upload_document_file_size_max, :size => 3 %> MB
    &nbsp;
    <%= raw(t("rumi.gwboard.message.temp_size_limit"))%>
  </td>
</tr>
</table>
