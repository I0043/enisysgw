<div class="bbsDocList circular">
  <%= render :partial => 'piece_header' %>
  <div class="pieceBody">
    <%= form_for :item, :url => "#{@public_uri}", :action => :create ,:html => {:method => :post , :multipart => true, :id => 'itemForm'} do |f| %>
    <%= f.error_messages %>
    <%= render :partial => 'form', :locals => {:f => f} %>
    <%= submit_for_update(f, :js_submitter => "return my_submit()") %>
    <% end %>
  </div><!--pieceBody-->
</div><!--bbsDocList circular-->
