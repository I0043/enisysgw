.header_img {
  display: block;
  width: 220px;
  height: 80px;
  background-image: url("/images/logo.jpg");
}
.schedule_print { margin-left: 10px; }
#errorExplanation {
  width: 500px;
  margin: 10px 0 15px 10px;
  border: 2px solid #f00;
}
#errorExplanation h2 {
  padding: 5px 10px;
  background-color: #c00;
  color: #ffffff;
  font-size: 100%;
}
#errorExplanation p {
  padding: 5px 10px;
  color: #333;
  font-weight: bold;
}
#errorExplanation ul {
  padding: 0px 0px 10px 0px;
  list-style-type: square;
}
#errorExplanation ul li {
  margin-left: 30px;
  padding-left: 0px;
  font-size: 12px;
}
.backlink {
  width: 200px;
  margin: 0 auto 1em;
}
.backlink ul {
  display: table;
  text-align: right;
  width: 100%;
  background-color: #000;
  border-radius: 5px;
}
.backlink ul li {
  display: table-cell;
  text-align: center;
  border-left: 1px solid #fff;
}
.backlink ul li:first-child { border-left: 0; }
.backlink ul li img {
  display: inline;
  vertical-align: middle;
}
.backlink ul li a {
  display: block;
  line-height: 40px;
  padding: 0 1em;
  color: #fff;
}
.backlink ul li:first-child a { border-radius: 5px 0 0 0; }
.backlink ul li:last-child a { border-radius: 0 5px 0 0; }
.paginationSingle ul li:first-child a { border-radius: 5px; }
.backlink ul li a:hover {
  background-color: #c5494c;
  color: #fff;
}
.configTable th, .configTable td {
  border-right-style: hidden !important;
  border-left-style: hidden !important;
}
.schedule-body * {
  background-repeat: no-repeat;
}
.schedule-body .username {
  margin-bottom: 3px;
}
.schedule-body .username.image img {
  min-width: 40px;
  min-height: 40px;
  max-width: 100px;
  max-height: 75px;
}
.schedule-body .username.other {
  background-image: url(/images/icon/ic_etc.gif);
  width: 32px;
  height: 34px;
}
.backlink ul li.disable_first {
  padding: 0 1em;
  border-top-left-radius: 5px;
  border-bottom-left-radius: 5px;
  background-color: #888;
}
.backlink ul li.disable_last {
  padding: 0 1em;
  border-top-right-radius: 5px;
  border-bottom-right-radius: 5px;
  background-color: #888;
}
.imgBox {
  float:left;
}
.imgBox img {
  display: block;
  width: 400px;
  margin-bottom: 15px;
  border: 1px solid #999999;
}
.noImage {
  width: 350px;
  height: 300px;
  border: 1px solid #999999;
}
.profile_noImage {
  width: 200px;
  height: 150px;
  border: 1px solid #999999;
  margin-bottom: 20px;
}
.tableBox {
    height: 1%;
    margin-left: 420px;
}
table.rentDetails th {
    width: 135px;
}
#header-users tr {
  height: 40px;
}
#header-users td.user a {
  text-align: center;
  vertical-align: middle;
  height: 40px;
  line-height: 40px;
}
.field_with_errors {
  display: inline-block;
}
.output_csv_note {
  margin-top: 20px;
  margin-bottom: 10px;
  line-height: 1.3;
}
.buttonlink {
  margin-bottom: 1em;
}
.buttonlink ul {
  display: table;
  background-color: #000;
  border-radius: 5px;
}
.buttonlink ul li {
  display: table-cell;
  text-align: center;
  border-left: 1px solid #fff;
}
.buttonlink ul li:first-child { border-left: 0; }
.buttonlink ul li img {
  display: inline;
  vertical-align: middle;
}
.buttonlink ul li a {
  display: block;
  line-height: 30px;
  padding: 0 1em;
  color: #fff;
}
.buttonlink ul li:first-child a { border-radius: 5px; }
.buttonlink ul li a:hover {
  background-color: #c5494c;
  color: #fff;
}
.ldap_dep {
  margin: 10px 0px 0px 0px;
  padding: 5px 5px;
  background-color: #bbb;
}
.ldap_sec {
  margin: 0px 0px 5px 0px;
  padding: 5px 5px;
  background-color: #ddd;
}
.ldap_users {
  margin: 5px 5px;
  padding: 5px;
  border: 0px solid #ccc;
  font-size: 90%;
}
.ldap_user {
  display: inline-block;
  margin: 0px 0.2em 0px 0px;
}
.highcharts-container{
  position: static !important;
}
