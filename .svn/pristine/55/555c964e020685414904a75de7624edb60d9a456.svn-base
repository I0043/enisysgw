<div class="main">
  <%= div_notice %>
  <%= form_for :item, url: "/gw/edit_link_pieces/#{@item.id}", action: :update, html: {method: :put, multipart: true} do |f| %>
    <%= f.error_messages %>
    <%= render partial: 'form', locals: {f: f} %>
    <%= submit_for f, {caption: t("rumi.submit.update")} %>
  <% end  %>
</div>
