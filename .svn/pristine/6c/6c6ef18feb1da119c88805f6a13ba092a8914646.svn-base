<%= render partial: 'piece_header' %>
<div class="main">
  <%= form_for :item, :url => "/gwcircular/basics/#{@item.id}", html: {method: :put, multipart: true, id: 'itemForm'} do |f| %>
    <%= f.error_messages %>
    <%= render partial: 'form', locals: {f: f} %>
    <%= submit_for_update(f, js_submitter: "return my_submit()") %>
  <% end %>
</div>
