


<div class="piece YearFiscalJp">
<%= render :partial => 'header_str' %>

<div class="pieceBody">

<%= form_for :item, :url => "/gw/year_fiscal_jps/#{@item.id}?#{@qs}", :html => {:method => :put} do |f| %>

