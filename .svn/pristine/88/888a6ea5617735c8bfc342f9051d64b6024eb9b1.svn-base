<%= render partial: 'piece_header' %>

<!-- コンテンツここから -->
<div class="main">
  <%= form_for :item, url: gw_prop_group_setting_path(@item.id), html: {method: :put, multipart: true, id: 'itemForm'} do |f| %>
    <%= f.error_messages %>
    <%= render partial: 'form', locals: {f: f} %>
    <%= raw(submit_for_update(f, js_submitter: "return my_submit()", class: :mockblock)) %>
  <% end %>
</div>
