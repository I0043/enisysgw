<%
  edit = t("rumi.link.edit")
  delete = t("rumi.link.delete")
  confirm = t("rumi.message.confirm.delete")
%>
<div class="area_contentsNav">
  <div class="navBlock">
    <div class="contentsNav">
      <ul>
        <li>
          <%= link_to image_tag("/images/icon/add.png", alt: edit) + edit, "/gw/admin_messages/#{@item.id}/edit?#{@qs}" %>
        </li>
        <li>
          <% del = t("rumi.button.delete") %>
          <%= link_to image_tag("/images/icon/dele.png", alt: delete) + delete, "/gw/admin_messages/#{@item.id}?#{@qs}", data: {confirm: confirm}, method: 'delete' %>
        </li>
      </ul>
    </div>
  </div>
</div>
