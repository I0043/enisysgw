<%= render partial: 'piece_header', locals: {new: false} %>

<!-- コンテンツここから -->
<div class="main">
  <%= form_for :item, url: gw_prop_type_path(@item.id), html: {method: :put, multipart: true, id: 'itemForm'} do |f| %>
    <%= f.error_messages %>
    <%= render partial: 'form', locals: {f: f} %>
    <div class="submitters">
      <%= f.submit t("rumi.submit.update") %>
    </div>
  <% end %>
</div>
<!-- コンテンツここまで -->
